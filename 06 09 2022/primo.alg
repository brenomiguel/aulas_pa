algoritmo "semnome"
// Função :
// Autor :
// Data : 06/09/2022
// Seção de Declarações 
var
primo, contador, n1 :inteiro
inicio
// Seção de Comandos 
para contador de 1 ate 100 faca
 primo <- 0
 para n1 de 1 ate contador faca
  se (contador mod n1=0)entao
   primo <- primo + 1
   fimse
  fimpara
  se (primo=2) entao
   escreva(contador)
  fimse
fimpara
fimalgoritmo
