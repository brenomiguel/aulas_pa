algoritmo "semnome"
// Função :
// Autor :
// Data : 20/09/2022
// Seção de Declarações 
var
contador,soma,qntd,id:inteiro
md:real
inicio
// Seção de Comandos
para contador de 1 ate 10 faca
 escreval("Digite sua idade")
 leia(id)

 se id >  entao
  escreval(id)
  soma <- soma+id
  qntd <- qntd+1
 fimse
fimpara
md <- soma/qntd

escreval("A média é", md)
fimalgoritmo
